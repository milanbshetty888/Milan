function myFunction() {
    //alert("The form was submitted");
    name=document.getElementById("name").value;
    email=document.getElementById("email").value;
    textarea=document.getElementById("textarea").value;
    if(name=="" && email=="" && textarea=="")
    {
       alert("Empty fields, enter name, email and review");
    }
    else 
    {
      if (name=="") 
        alert("Empty field, enter password");


      if (email=="") 
        alert("Empty field, enter email");

      if (textarea=="") 
        alert("Empty field, enter review");


    }
}


