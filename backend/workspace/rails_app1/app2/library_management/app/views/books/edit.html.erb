<%= form_for :book, url: book_edit_path(@book), method: :put do |f|%>
	<p>
		<%= f.label :book_name %>
		<%= f.text_field :book_name %>
	</p>
	<p>
		<%= f.label :author %>
		<%= f.text_field :author %>
	</p>
	<p>
		<%= f.label :publication %>
		<%= f.text_field :publication %>
	</p>

	<p>
		<%= f.submit%>
	</p>
<%end%>